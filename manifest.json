{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MyParcel/PostNL shipping label creator",
  "version": "2.0.0",
  "icons": {
    "16": "dist/images/icon-16px.png",
    "48": "dist/images/icon-48px.png",
    "128": "dist/images/icon-128px.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "externally_connectable": {
    "matches": [
      "*://chrome.test/*"
    ]
  },
  "permissions": [
    "chrome-extension://*",
    "storage",
    "webNavigation",
    "tabs",
    "notifications",
    "create",
    "*://*/*",
    "http://localhost/*",
    "contextMenus",
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "./dist/js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "dist/images/icon-128px.png"
  }
}
