{
  "name": "myparcel-chrome-extension",
  "version": "2.4.3",
  "description": "MyParcel Chrome Extension",
  "repository": {
    "url": "https://git.dmp.zone/projects/MYP/repos/chrome-extension"
  },
  "scripts": {
    "dev": "webpack-dev-server --env.NODE_ENV=development --config build/webpack.dev.conf.js",
    "prod": "webpack --config build/webpack.prod.conf.js && node private/process.js --upload --publish",
    "prod:build": "webpack --config build/webpack.prod.conf.js",
    "prod:upload": "node private/process.js --upload",
    "prod:publish": "node private/process.js --publish",
    "release": "npm --no-git-tag-version version minor && npm run prod:build && node private/process.js --build prod --upload --publish",
    "hotfix": "npm --no-git-tag-version version patch && npm run prod:build && node private/process.js --build prod --upload --publish",
    "stage": "webpack --config build/webpack.stage.conf.js && node private/process.js --build stage --upload --publish",
    "stage:build": "webpack --env.NODE_ENV=staging --config build/webpack.stage.conf.js",
    "stage:upload": "node private/process.js --build stage --upload",
    "stage:publish": "node private/process.js --build stage --publish",
    "upload": "node private/process.js --upload",
    "publish": "node private/process.js --publish",
    "test": "jest"
  },
  "author": "Edie Lemoine",
  "license": "ISC",
  "dependencies": {
    "unique-selector": "^0.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@myparcel/eslint-config": "^1.0.1",
    "@types/chrome": "0.0.89",
    "@types/jest": "^24.0.18",
    "babel-loader": "^8.0.6",
    "chrome-webstore-upload": "^0.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.3.0",
    "jest-webextension-mock": "^3.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mustache-loader": "^1.4.3",
    "node-sass": "^4.13.0",
    "puppeteer": "^1.20.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-strip-log-loader": "^0.1.11",
    "yargs": "^14.0.0"
  }
}
