{
  "name": "myparcel-chrome-extension",
  "version": "2.3.0",
  "description": "MyParcel Chrome Extension",
  "repository": {
    "url": "https://git.dmp.zone/projects/MYP/repos/chrome-extension"
  },
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.dev.conf.js",
    "prod": "webpack --config build/webpack.prod.conf.js && node private/process.js --upload --publish",
    "prod:build": "webpack --config build/webpack.prod.conf.js",
    "prod:upload": "node private/process.js --upload",
    "prod:publish": "node private/process.js --publish",
    "release": "npm --no-git-tag-version version minor && npm run build && node private/process.js --build prod --upload --publish",
    "hotfix": "npm --no-git-tag-version version patch && npm run build && node private/process.js --build prod --upload --publish",
    "stage": "webpack --config build/webpack.stage.conf.js && node private/process.js --build stage --upload --publish",
    "stage:build": "webpack --config build/webpack.stage.conf.js",
    "stage:upload": "node private/process.js --build stage --upload",
    "stage:publish": "node private/process.js --build stage --publish",
    "upload": "node private/process.js --upload",
    "publish": "node private/process.js --publish",
    "test": "jest"
  },
  "author": "Edie Lemoine",
  "license": "ISC",
  "dependencies": {
    "unique-selector": "^0.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@types/chrome": "0.0.83",
    "@types/jest": "^24.0.13",
    "babel-loader": "^8.0.6",
    "chrome-webstore-upload": "^0.2.2",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-puppeteer": "^4.2.0",
    "jest-webextension-mock": "^3.5.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mustache-loader": "^1.4.3",
    "node-sass": "^4.12.0",
    "puppeteer": "^1.17.0",
    "sass-loader": "^7.1.0",
    "sinon-chrome": "^3.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-strip-log-loader": "^0.1.5",
    "yargs": "^13.2.4"
  }
}
