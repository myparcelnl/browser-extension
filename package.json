{
  "name": "@myparcel/chrome-extension",
  "version": "2.7.2",
  "description": "MyParcel Chrome Extension",
  "repository": {
    "url": "git+ssh://git@github.com/myparcelnl/browser-extension.git"
  },
  "license": "MIT",
  "author": "Edie Lemoine <edie@myparcel.nl>",
  "scripts": {
    "dev": "NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server --env.NODE_ENV=development --config build/webpack.dev.conf.js",
    "hotfix": "npm --no-git-tag-version version patch && run prod:build && node private/process.mjs --build prod --upload --publish",
    "prod": "NODE_OPTIONS=--openssl-legacy-provider webpack --config build/webpack.prod.conf.js && node private/process.mjs --upload --publish",
    "prod:build": "NODE_OPTIONS=--openssl-legacy-provider webpack --config build/webpack.prod.conf.js",
    "prod:publish": "node private/process.mjs --build prod --publish",
    "prod:upload": "node private/process.mjs --build prod --upload",
    "publish": "node private/process.mjs --publish",
    "release": "npm --no-git-tag-version version minor && run prod:build && node private/process.mjs --build prod --upload --publish",
    "stage": "run stage:build && node private/process.mjs --build stage --upload --publish",
    "stage:build": "NODE_OPTIONS=--openssl-legacy-provider webpack --env.NODE_ENV=staging --config build/webpack.stage.conf.js",
    "stage:publish": "node private/process.mjs --build stage --publish",
    "stage:upload": "node private/process.mjs --build stage --upload",
    "test": "jest",
    "upload": "node private/process.mjs --upload"
  },
  "dependencies": {
    "unique-selector": "^0.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.12",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.17.9",
    "@myparcel/eslint-config": "^3.4.4",
    "@types/chrome": "0.0.186",
    "@types/jest": "^25.2.3",
    "babel-loader": "^8.2.5",
    "chrome-webstore-upload": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "jest-puppeteer": "^6.1.0",
    "jest-webextension-mock": "^3.6.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mustache-loader": "^1.4.3",
    "puppeteer": "^14.1.0",
    "sass": "^1.51.0",
    "sass-loader": "^10.2.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-strip-log-loader": "^0.3.0",
    "yargs": "^15.3.1"
  },
  "packageManager": "yarn@4.1.1",
  "volta": {
    "node": "20.12.2"
  }
}
